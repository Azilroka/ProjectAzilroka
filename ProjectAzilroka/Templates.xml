			<Layer level='OVERLAY' textureSubLevel='1'>
				<FontString parentKey='unitName' inherits='GameFontHighlightSmallOutline'>
					<Anchors>
							<Anchor point='BOTTOM' relativeKey='$parent' relativePoint='TOP'/>
